<nav class="navbar navbar-toggleable-md navbar-light bg-faded" style="background-color: orange;">
  <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <a class="navbar-brand" href="http://www.clarientglobal.com/"><img src="./../assets/clarient-logo.png" alt="Clarient Entity Hub" /></a>
  <div class="collapse navbar-collapse" id="navbarNavDropdown">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link" routerLink="." routerLinkActive="active">Dashboard <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          Entity Records
        </a>
        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
          <a class="dropdown-item" routerLink="/my-entities" routerLinkActive="active">My Entities</a>
          <a class="dropdown-item" routerLink="." routerLinkActive="active">Create Record</a>
          <a class="dropdown-item" routerLink="." routerLinkActive="active">Manage Bulk</a>
        </div>
      </li>
      <li class="nav-item">
        <a class="nav-link disabled" href="#" disabled="disabled">Admin</a>
      </li>
    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="text" placeholder="Search">
      <button class="btn btn-outline-warning my-2 my-sm-0" type="submit">Search</button>
    </form>
  </div>
</nav>


